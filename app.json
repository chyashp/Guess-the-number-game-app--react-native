{
  "expo": {
    "name": "Guess My Number",
    "slug": "guess-my-number",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dice.webp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/dice.webp",
      "resizeMode": "contain",
      "backgroundColor": "#72063c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/dice.webp",
        "resizeMode": "contain",
        "backgroundColor": "#72063c"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dice.webp",
        "backgroundColor": "#72063c"
      },
      "splash": {
        "image": "./assets/images/dice.webp",
        "resizeMode": "contain",
        "backgroundColor": "#72063c"
      }
    },
    "web": {
      "favicon": "./assets/images/dice.webp"
    }
  }
}
